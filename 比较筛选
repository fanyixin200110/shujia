#include <stdio.h>


int main()
{
    int x,y;
    printf(" 输入当前速度和限速\n");
    scanf ("%d%d",&x,&y);
      if (x> y*1.5)
        {
            printf ("Revoke driver's license");
        }
       else if (x> y*1.1)

{
       printf ("A fine of 200 yuan");
}
           else
{
        printf ("no problem");
}

}


